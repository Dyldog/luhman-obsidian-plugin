{
    "name": "luhman",
    "version": "1.1.3",
    "description": "Commands for handling a zettelkasten with Luhmann-style IDs as filenames",
    "main": "main.js",
    "scripts": {
        "prettier": "prettier --write 'src/**/*.+(ts|tsx|json|html|css)'",
        "lint": "eslint . --ext .ts,.tsx --fix",
        "dev": "rollup --config rollup.config.js -w",
        "release": "dotenv release-it --'plugins.@release-it/bumper.out=\"manifest.json\"'",
        "release-beta": "dotenv release-it --'plugins.@release-it/bumper.out=\"manifest-beta.json\"'",
        "build": "rollup --config rollup.config.js --environment BUILD:production"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@release-it/bumper": "^2.0.0",
        "@rollup/plugin-commonjs": "^18.0.0",
        "@rollup/plugin-node-resolve": "^11.2.1",
        "@rollup/plugin-typescript": "^8.2.1",
        "@types/json-schema": "^7.0.7",
        "@types/node": "^14.14.37",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "assert-never": "^1.2.1",
        "dotenv-cli": "^6.0.0",
        "eslint": "^8.23.0",
        "json": "^11.0.0",
        "obsidian": "^0.12.0",
        "path-browserify": "^1.0.1",
        "prettier": "^2.3.2",
        "rollup": "^2.32.1",
        "rollup-plugin-import-css": "^2.0.1",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4"
    }
}
